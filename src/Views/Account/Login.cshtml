@using GoldenTicket.Models.AccountViewModels 
@model LoginRequest 
@{ 
	ViewData["Title"] = "Login";
}

<form method="post">
		<input asp-for="Username" placeholder="Username">
		<input asp-for="Password" class="input100" placeholder="Password">
		<input asp-for="RememberMe">
		<button type="submit">
			Login
		</button>
</form>